{% layout "layout.liquid" %} {% block content %}
{% for post in posts %}
<article>
    <div class="item-div">
      <h3>
        {{ post.name }}
      </h3>
    <a href="/posts/feed/{{ post._id }}">
      <img src="{{ post.image_url }}.png" width='200px'/>
    </a>
</article>
{% endfor %}
 {% endblock %}